package com.kosta.exam;

public class D29MethodGetFactorial {
	
	//정수형 변수 n을 전달받아서 n!을 구하여 반환
	public static int getFactori(int n) {
		int num =1;
		for(int i =1; i <= n; i++) {
			num *= i;
		}		
		return num;
	}
	
	//메인 메서드 
	public static void main(String[] args) {
		System.out.println(getFactori(5));
	}

}
