package com.kosta.exam;

import java.util.Arrays;

public class D06ArrayCopyTest {
	public static void main(String[] args) {
		int[] a = {1, 2, 3, 4, 5};
		int[] b = new  int[5];
		b = a;
		
		b[1] = 100;
		
		a[0] = 100;
		
		System.out.println(Arrays.toString(a));
		System.out.println(Arrays.toString(a));
		
	}
}
