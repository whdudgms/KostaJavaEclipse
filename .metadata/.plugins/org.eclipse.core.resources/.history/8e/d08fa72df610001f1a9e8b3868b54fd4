package day0513;

import java.util.Scanner;

public class D22_MaxName {
	
	Scanner sc = new Scanner(System.in);
	
	int[] score = new int[5];
	String[] name = new String[];
	
	for(int i = 0; i < score.length; i++) {
		System.out.println("이름을 입력해 주새요 ");
		name[i] = sc.next();
		System.out.println("점수를 입력해 주새요 ");
		score[i] = sc.nextInt();
	}

}
